#include<time.h>
#include<iostream>
#include<stdlib.h>
using namespace std;
int main(){
    int fil, col;
    int num;
    srand(time(0));
    int matrix[3][3];
for(fil=0;fil<3;fil++){
        for(col=0;col<3;col++){
             matrix[fil][col]=rand()%2;
             cout<<matrix[fil][col];
             cout<<" ";
             }
             cout<<"\n";
             }
   


cin.get();
cin.get();
}
